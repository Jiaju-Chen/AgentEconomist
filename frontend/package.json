{
  "name": "langgraph-python-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run dev:ui",
    "dev:with-agent": "concurrently \"npm run dev:ui\" \"npm run dev:agent\" --names ui,agent --prefix-colors blue,green --kill-others",
    "dev:debug": "LOG_LEVEL=debug npm run dev",
    "dev:agent": "cd agent && conda run -n economist langgraph dev --host 127.0.0.1 --port 8123 --no-browser",
    "dev:ui": "next dev --turbopack -p 3001",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "install:agent": "sh ./scripts/setup-agent.sh || scripts\\setup-agent.bat",
    "postinstall": "npm run install:agent"
  },
  "dependencies": {
    "@copilotkit/react-core": "1.50.1",
    "@copilotkit/react-ui": "1.50.1",
    "@copilotkit/runtime": "1.50.1",
    "antd": "^6.1.3",
    "diff": "^8.0.2",
    "next": "16.1.1",
    "openai": "^5.23.2",
    "react": "^19.2.3",
    "react-diff-view": "^3.3.2",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@langchain/langgraph-cli": "^1.1.2",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.2",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
